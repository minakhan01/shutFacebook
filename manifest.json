{
    "manifest_version": 3,
    "name": "Domain Blocker",
    "version": "1.0",
    "description": "Blocks specified domains during a set interval.",
    "permissions": ["tabs", "storage", "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon_128.png"
    }
  }
  